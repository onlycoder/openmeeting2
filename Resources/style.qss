/*slider*/
QSlider
{                             
	width: 100px;
	height:16px;                                           

}
QSlider::groove:horizontal
{
	border: 0px solid #999999;                             
	width:100px;
	height:1px;                                           
	margin: 1px 0;                                         
	left: 12px; right: 12px;
}
QSlider::handle:horizontal
{  
	border-image: url(:/slider/slider/thumb.png) 0 12 0 12;
	width:12px;
	height:19Px;
	margin: -9px 0px -9px 0px;
}  

QSlider::handle:horizontal:hover
{  
	border-image: url(:/slider/slider/thumb.png) 0 24 0 0;
}  

QSlider::handle:horizontal:pressed
{  
	border-image: url(:/slider/slider/thumb.png) 0 0 0 24;
} 

QSlider::add-page:horizontal 
{
    background: rgb(202,206,200);
}

QSlider::sub-page:horizontal
{
    background:rgb(101,187,51);
}

