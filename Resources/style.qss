/***********************ScrollBar**********************/
QScrollBar:vertical
{
	background: transparent;
	margin: 0px 0px 0px 0px;
	min-width: 8px;
	max-width: 8px;
}

QScrollBar:vertical:hover, QScrollBar:vertical:pressed
{
	border-image: url(:skin/scroll/style_2_hover.png) 15 0 15 0;
	border-width: 15px 0px 15px 0px;
	padding: -15 0 -15 0;
	min-height:30px;
	min-width: 8px;
	max-width: 8px;
}

QScrollBar::handle:vertical
{
	border-image: url(:skin/scroll/style_1_normal.png) 5 0 5 0;
	border-width: 5px 0px 5px 0px;
	margin: 10px 0px 10px 0px;
	min-height:30px;
	min-width: 8px;
	max-width: 8px;
}

QScrollBar::handle:vertical:hover, QScrollBar::handle:vertical:active
{
	border-image: url(:skin/scroll/style_1_hover.png) 5 0 5 0;
	border-width: 5px 0px 5px 0px;
}

QScrollBar::handle:vertical:pressed
{
	border-image: url(:skin/scroll/style_1_hover.png) 5 0 5 0;
	border-width: 5px 0px 5px 0px;
}

QScrollBar::add-line:vertical
{
	width: 8px;
	height: 10px;
	subcontrol-position: bottom;
	background: transparent;
}

QScrollBar::sub-line:vertical
{
	width: 8px;
	height: 10px;
	subcontrol-position: top;
	background: transparent;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical, QListWidget QScrollBar::sub-page:vertical, QTabCtrl QScrollBar::sub-page:vertical
{
	background: none;
}


